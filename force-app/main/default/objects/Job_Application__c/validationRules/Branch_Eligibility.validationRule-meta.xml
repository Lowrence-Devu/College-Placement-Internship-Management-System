<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Branch_Eligibility</fullName>
    <active>true</active>
    <errorConditionFormula>NOT(ISPICKVAL(Student__r.Branch__c, &quot;CSE&quot;) ||
    ISPICKVAL(Student__r.Branch__c, &quot;ECE&quot;) ||
    ISPICKVAL(Student__r.Branch__c, &quot;IT&quot;))</errorConditionFormula>
    <errorMessage>“Student branch not eligible for this job.”</errorMessage>
</ValidationRule>
